{
  "extends": "airbnb",
  "plugins": [ "jest" ],
  "env": {
    "jest/globals": true,
    "browser": true,
    "node": true,
  },
  "rules": {
    "linebreak-style": "off",
    "react/prop-types": [0],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}